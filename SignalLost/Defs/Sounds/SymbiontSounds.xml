<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Ambient Symbiont Sounds -->
  <SoundDef>
    <defName>SymbiontPulse</defName>
    <maxVoices>3</maxVoices>
    <maxSimultaneous>2</maxSimultaneous>
    <sustainLooping>false</sustainLooping>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Sounds/FleshSymbiont</clipFolderPath>
            <clipPrefix>SymbiontPulse</clipPrefix>
          </li>
        </grains>
        <volumeRange>0.3~0.6</volumeRange>
        <pitchRange>0.85~1.15</pitchRange>
        <distRange>3~25</distRange>
      </li>
    </subSounds>
  </SoundDef>

